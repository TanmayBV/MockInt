from sqlalchemy.orm import Session
from app import models, schemas

def create_result(db: Session, result: schemas.InterviewResultCreate):
    db_result = models.InterviewResult(**result.dict())
    db.add(db_result)
    db.commit()
    db.refresh(db_result)
    return db_result

def get_all_results(db: Session):
    return db.query(models.InterviewResult).all()
